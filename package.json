{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build && npm run copy-manifest",
        "watch": "vite build --watch",
        "prod": "vite build && npm run copy-manifest",
        "production": "vite build && npm run copy-manifest",
        "copy-manifest": "node -e \"const fs=require('fs');const path=require('path');const src=path.join('public','build','.vite','manifest.json');const dest=path.join('public','build','manifest.json');if(fs.existsSync(src)){fs.copyFileSync(src,dest);console.log('Manifest copied to public/build/manifest.json');}else{console.log('Source manifest not found');}\"",
        "minify-assets": "node scripts/minify-assets.js"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@tailwindcss/forms": "^0.5.11",
        "alpinejs": "^3.15.3",
        "autoprefixer": "^10.4.23",
        "axios": "^1.7.9",
        "bootstrap": "^5.3.3",
        "bootstrap-datepicker": "^1.10.1",
        "feather-icons": "^4.29.2",
        "jquery": "^3.7.1",
        "jquery.nicescroll": "^3.7.6",
        "laravel-vite-plugin": "^1.1.0",
        "lodash": "^4.17.5",
        "postcss": "^8.4.31",
        "sticky-kit": "^1.1.3",
        "swiper": "^11.2.10",
        "tailwindcss": "^3.1.0",
        "terser": "^5.44.0",
        "tom-select": "^2.4.3",
        "ttf2woff2": "^8.0.0",
        "vite": "^6.0.0"
    },
    "dependencies": {
        "@fullcalendar/core": "^6.1.20",
        "@fullcalendar/daygrid": "^6.1.20",
        "@fullcalendar/interaction": "^6.1.20",
        "@fullcalendar/list": "^6.1.20",
        "@fullcalendar/timegrid": "^6.1.20",
        "tinymce": "^8.3.1"
    }
}
